int led = 13;
int AIN1 = 7;
int AIN2 = 8;
int BIN1 = 9;
int BIN2 = 10;
int motorPWMA = 3;
int motorPWMB = 5;

void setup() {
  pinMode(led, OUTPUT);
  pinMode(AIN1, OUTPUT);
  pinMode(AIN2, OUTPUT);
  pinMode(BIN1, OUTPUT);
  pinMode(BIN2, OUTPUT);
  pinMode(A0, INPUT);
  Serial.begin(19200);
  Serial.println("start up");


}

void forward(int howfast) {
  digitalWrite(led, HIGH);
  digitalWrite(AIN1, HIGH);
  digitalWrite(AIN2, LOW);
  digitalWrite(BIN1, HIGH);
  digitalWrite(BIN2, LOW);

  analogWrite(motorPWMA, howfast);
  analogWrite(motorPWMB, howfast);

  Serial.println(howfast);

}

void loop() {
  forward(128);
  delay(1500);
  forward(200);
  delay(1500);
  forward(255);
  delay(1500);
 

}
